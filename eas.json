{
  "cli": {
    "version": ">= 3.13.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "env": {
        "API_KEY": "development_api_key",
        "BASE_URL": "https://api-dev.yourservice.com"
      }
    },
    "preview": {
      "env": {
        "API_KEY": "preview_api_key",
        "BASE_URL": "https://api-staging.yourservice.com"
      }
    },
    "production": {
      "node": "20.17.0",
      "autoIncrement": true,
      "distribution": "store",
      "env": {
        "API_KEY": "production_api_key",
        "BASE_URL": "https://api.yourservice.com"
      }
    },
    "e2e": {
      "withoutCredentials": true,
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6738967491"
      }
    }
  }
}
